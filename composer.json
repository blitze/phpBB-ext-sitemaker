	"version": "3.3.1",
	"time": "2020-11-16",
	"keywords": [
		"phpbb",
		"extension",
		"portal",
		"cms"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Daniel A.",
			"email": "shizzah@hotmail.com",
